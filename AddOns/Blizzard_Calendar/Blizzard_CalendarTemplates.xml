<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<Texture name="CalendarDarkDayTopTextureTemplate" file="Interface\Calendar\CalendarShadows" virtual="true">
		<Size x="91" y="46"/>
	</Texture>
	<Texture name="CalendarDarkDayBottomTextureTemplate" file="Interface\Calendar\CalendarShadows" virtual="true">
		<Size x="91" y="45"/>
	</Texture>
	<Button name="CalendarDayButtonTemplate" virtual="true">
		<Size x="91" y="91"/>
		<Anchors>
			<Anchor point="TOPLEFT"/>
		</Anchors>
		<Layers>
			<Layer level="BORDER">
				<Texture name="$parentEventTexture" hidden="true">
					<Size x="91" y="91"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<TexCoords left="0.0" right="0.796875" top="0.0" bottom="0.71875"/>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentEventBackgroundTexture" file="Interface\Calendar\CalendarEventBackground" hidden="true">
					<Size x="91" y="86"/>
					<Anchors>
						<Anchor point="BOTTOM"/>
					</Anchors>
					<TexCoords left="0.0" right="0.671875" top="0.0" bottom="0.671875"/>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture name="$parentPendingInviteTexture" file="Interface\Calendar\EventNotification" hidden="true">
					<Size x="71" y="91"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<TexCoords left="0.03125" right="0.6484375" top="0.03125" bottom="0.8671875"/>
				</Texture>
			</Layer>
		</Layers>
		<NormalTexture file="Interface\Calendar\CalendarBackground"/>
		<HighlightTexture file="Interface\Calendar\Highlights" alphaMode="ADD">
			<Size x="88" y="88"/>
			<Anchors>
				<Anchor point="CENTER"/>
			</Anchors>
			<TexCoords left="0.0" right="0.34375" top="0.0" bottom="0.6875"/>
		</HighlightTexture>
		<Frames>
			<Frame name="$parentDateFrame">
				<Size x="23" y="18"/>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="5" y="-5"/>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture name="$parentBackground" file="Interface\Calendar\DateBackgrounds" setAllPoints="true" hidden="true">
							<TexCoords left="0.0" right="0.359375" top="0.625" bottom="0.921875"/>
							<Color r="1.0" g="1.0" b="1.0" a="0.8"/>
						</Texture>
					</Layer>
					<Layer level="ARTWORK">
						<FontString name="$parentDate" inherits="GameFontHighlight" justifyH="LEFT">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
			<Button name="$parentMoreEventsButton" hidden="true">
				<Size x="16" y="16"/>
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset x="-5" y="-7"/>
					</Anchor>
				</Anchors>
				<NormalTexture file="Interface\Calendar\MoreArrow"/>
				<Scripts>
					<OnLoad>
						CalendarDayButtonMoreEventsButton_OnLoad(self);
					</OnLoad>
					<OnEnter>
						CalendarDayButtonMoreEventsButton_OnEnter(self);
					</OnEnter>
					<OnLeave>
						CalendarDayButtonMoreEventsButton_OnLeave(self);
					</OnLeave>
					<OnClick>
						CalendarDayButtonMoreEventsButton_OnClick(self, button);
					</OnClick>
				</Scripts>
			</Button>
			<Frame name="$parentDarkFrame" hidden="true">
				<Size x="91" y="91"/>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture name="$parentTop" file="Interface\Calendar\CalendarShadows">
							<Size x="91" y="46"/>
							<Anchors>
								<Anchor point="TOP">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1.0" g="1.0" b="1.0" a="0.9"/>
						</Texture>
						<Texture name="$parentBottom" file="Interface\Calendar\CalendarShadows">
							<Size x="91" y="45"/>
							<Anchors>
								<Anchor point="BOTTOM">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1.0" g="1.0" b="1.0" a="0.9"/>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnLoad>
						self:SetFrameLevel(self:GetParent():GetFrameLevel() + 2);
					</OnLoad>
				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				CalendarDayButton_OnLoad(self);
			</OnLoad>
			<OnEnter>
				CalendarDayButton_OnEnter(self);
			</OnEnter>
			<OnLeave>
				CalendarDayButton_OnLeave(self);
			</OnLeave>
			<OnClick>
				CalendarDayButton_OnClick(self, button);
			</OnClick>
		</Scripts>
	</Button>
	<Button name="CalendarDayEventButtonTemplate" virtual="true" hidden="true">
