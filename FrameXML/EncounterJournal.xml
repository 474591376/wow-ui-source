	<Button name="EncounterCreatureButtonTemplate" motionScriptsWhileDisabled="true" virtual="true" hidden="true">
		<Size x="100" y="55"/>
		<Frames>
			<Frame>
				<Size x="1" y="1"/>
				<Anchors>
					<Anchor point="TOPRIGHT" x="0" y="0"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY" TextureSubLevel="6">
						<Texture name="$parentCreature" file="Interface\EncounterJournal\UI-EJ-BOSS-Default" parentKey="creature">
							<Size x="128" y="64"/>
							<Anchors>
								<Anchor point="TOPRIGHT" x="24" y="13"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnLoad>
						self:GetParent().creature = self.creature;
					</OnLoad>
				</Scripts>
			</Frame>
		</Frames>
		<NormalTexture inherits="UI-EJ-BossButton-Up">
		</NormalTexture>
		<PushedTexture inherits="UI-EJ-BossButton-Down">
		</PushedTexture>
		<HighlightTexture inherits="UI-EJ-BossButton-Highlight">
		</HighlightTexture>
		<Scripts>
			<OnLoad>
				--Messy Pathwerk use to automatically update texcodors
				local ratio = 100/276 --New Size/Texture Size
				local texs = { self:GetNormalTexture(), self:GetPushedTexture(), self:GetHighlightTexture() };
				for i,tex in pairs(texs) do
					local l,t,_,b,r = tex:GetTexCoord();
					tex:SetTexCoord( (r-l)*ratio ,l,t,b);
				end
				
				--start small
				self:SetSize(70, 44);
				self.creature:SetSize(100, 50);
				self.creature:SetPoint("TOPRIGHT", self, "TOPRIGHT", 21, 8);
			</OnLoad>
			<OnDisable>
				self:SetSize(100, 55);
				self.creature:SetSize(128, 64);
				self.creature:SetPoint("TOPRIGHT", self, "TOPRIGHT", 24, 13);
			</OnDisable>
			<OnEnable>
				self:SetSize(70, 44);
				self.creature:SetSize(100, 50);
				self.creature:SetPoint("TOPRIGHT", self, "TOPRIGHT", 21, 8);
			</OnEnable>
			<OnClick>
				EncounterJournal_DisplayCreature(self)
			</OnClick>
			<OnEnter>
				if self.description then
					GameTooltip:SetOwner(self);
					GameTooltip:SetText(self.description);
				end
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>
	
