				</Scripts>
			</Frame>
		</Frames>		
		<Scripts>
			<OnLoad function="WatchFrame_OnLoad"/>
			<OnEvent function="WatchFrame_OnEvent"/>
			<OnUpdate function="WatchFrame_OnUpdate"/>
		</Scripts>
	</Frame>
	
